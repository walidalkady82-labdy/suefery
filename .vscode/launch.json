{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
 "configurations": [
        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "suefery",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "suefery (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "suefery (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
        "name": "Attach to Functions Emulator",
        "type": "node",
        "request": "attach",
        "port": 9229, // This is the default port Firebase uses
        "restart": true, // Automatically try to re-attach if the emulator restarts
        "skipFiles": [
            "<node_internals>/**" // Skips deep, internal Node.js files
        ]
        }
    ]

}